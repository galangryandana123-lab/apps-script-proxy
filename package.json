{
  "name": "akses-shortener",
  "version": "2.0.0",
  "description": "Multi-tenant URL shortener for Google Apps Script - Remove warning banner with custom slugs",
  "type": "module",
  "main": "api/[slug].js",
  "scripts": {
    "dev": "vercel dev",
    "build": "echo 'No build required - serverless functions project'",
    "vercel-build": "echo 'No build required'",
    "deploy": "vercel --prod",
    "test": "vitest --run --coverage"
  },
  "keywords": [
    "url-shortener",
    "proxy",
    "vercel",
    "serverless",
    "apps-script",
    "multi-tenant"
  ],
  "author": "Galang Ryandana",
  "license": "MIT",
  "dependencies": {
    "@vercel/kv": "^3.0.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^2.0.4",
    "vercel": "^39.0.0",
    "vitest": "^2.0.4"
  },
  "overrides": {
    "glob": "^11.0.0",
    "inflight": "npm:noop3@latest",
    "uuid": "^11.0.3"
  }
}
